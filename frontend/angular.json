{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/static"

            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "frontend:build:production"
            },
            "development": {
              "buildTarget": "frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/static"

            ],
            "styles": [
              "src/styles.css",
              "src/static/css/bootstrap1.min.css" ,
"src/static/vendors/themefy_icon/themify-icons.css" ,
"src/static/vendors/niceselect/css/nice-select.css" ,
"src/static/vendors/owl_carousel/css/owl.carousel.css" ,
"src/static/vendors/gijgo/gijgo.min.css" ,
"src/static/vendors/font_awesome/css/all.min.css" ,
"src/static/vendors/tagsinput/tagsinput.css" ,
"src/static/vendors/datepicker/date-picker.css" ,
"src/static/vendors/datatable/css/jquery.dataTables.min.css" ,
"src/static/vendors/datatable/css/responsive.dataTables.min.css" ,
"src/static/vendors/datatable/css/buttons.dataTables.min.css" ,
"src/static/vendors/text_editor/summernote-bs4.css" ,
"src/static/vendors/morris/morris.css" ,
"src/static/vendors/material_icon/material-icons.css" ,
"src/static/css/metisMenu.css" ,
"src/static/css/style1.css" ,
"src/static/css/colors/default.css" ,
"src/static/vendors/scroll/scrollable.css" ,

"src/static/vendors/calender_js/core/main.css" ,
"src/static/vendors/calender_js/list/main.css" ,
"src/static/vendors/animation/wow/animate.css"


            ],
            "scripts": ["node_modules/apexcharts/dist/apexcharts.min.js",
            "src/static/vendors/datatable/js/pdfmake.min.js",
            "src/static/vendors/datatable/js/vfs_fonts.js",
            "src/static/vendors/datatable/js/jquery.dataTables.min.js",
            "src/static/vendors/datatable/js/jszip.min.js",
            "src/static/vendors/datatable/js/buttons.print.min.js",
            "src/static/vendors/datatable/js/buttons.html5.min.js",
            "src/static/vendors/datatable/js/dataTables.buttons.min.js",
            "src/static/vendors/datatable/js/dataTables.responsive.min.js",
            "src/static/vendors/datatable/js/buttons.flash.min.js",

"src/static/vendors/calender_js/core/main.js",
"src/static/vendors/drag/jquery.ui.min.js",
"src/static/vendors/datepicker/datepicker.custom.js",
"src/static/vendors/chartjs/chartjs_init.js",
