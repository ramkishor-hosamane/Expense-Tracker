"src/static/js/custom.js",

"src/static/js/bootstrap1.min.js",

"src/static/vendors/scroll_reval/modernizr.js",

"src/static/vendors/jquery-sparkline/spactkine_init.js",
"src/static/vendors/tagsinput/tagsinput.js",
"src/static/vendors/owl_carousel/js/owl.carousel.min.js",
"src/static/vendors/am_chart/export.min.js",
"src/static/vendors/am_chart/pie.js",
"src/static/vendors/text_editor/summernote-bs4.js",
"src/static/js/popper1.min.js",
"src/static/vendors/scroll/scrollable-custom.js",
"src/static/vendors/chartjs/roundedBar.min.js",
"src/static/vendors/webticker/jquery.webticker.min.js",
"src/static/vendors/animation/wow/wow.min.js",
"src/static/vendors/count_up/jquery.counterup.min.js",
"src/static/vendors/chart_am/chart-custom.js"
            ]
          }
        }
      }
    }
  }
}
